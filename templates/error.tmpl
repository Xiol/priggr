{{ define "error" }}
  {{ if .ErrorMsg }}
      <div class="row">
          <div class="col-md-10 col-md-offset-1">
              <div class="alert alert-warning text-center" role="alert" id="pasteError">
                  <p>{{ .ErrorMsg }}</p>
              </div>
          </div>
      </div>
  {{ end }}
{{ end }}
